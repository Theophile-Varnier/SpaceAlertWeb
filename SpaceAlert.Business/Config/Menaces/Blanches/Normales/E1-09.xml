<objects xmlns="http://www.springframework.net"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/schema/objects/spring-objects-1.1.xsd" >
  <object id="Amoeba" type="SpaceAlert.Model.Menaces.MenaceExterne" singleton="false">
    <property name="Name" value="Amoeba"/>
    <property name="Shield" value="0"/>
    <property name="CurrentHp" value="8"/>
    <property name="MaxHp" value="8"/>
    <property name="Speed" value="2"/>
    <property name="AttackValues">
      <dictionary key-type="SpaceAlert.Model.Helpers.Enums.TypeCase">
        <entry key="Z">
          <list element-type="int">
            <value>5</value>
          </list>
        </entry>
      </dictionary>
    </property>
    <property name="HealValues">
      <dictionary key-type="SpaceAlert.Model.Helpers.Enums.TypeCase">
        <entry key="X" value="2"/>
        <entry key="Y" value="2"/>
      </dictionary>
    </property>
    <property name="Actions">
      <dictionary key-type="SpaceAlert.Model.Helpers.Enums.TypeCase">
        <entry key="X">
          <list>
            <ref object="Heal" />
          </list>
        </entry>
        <entry key="X">
          <list>
            <ref object="Heal" />
          </list>
        </entry>
        <entry key="X">
          <list>
            <ref object="Attack" />
          </list>
        </entry>
      </dictionary>
    </property>
  </object>
</objects>
