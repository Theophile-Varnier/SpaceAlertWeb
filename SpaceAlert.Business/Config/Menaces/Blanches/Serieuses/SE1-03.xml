<objects xmlns="http://www.springframework.net"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/schema/objects/spring-objects-1.1.xsd" >
  <object id="LeviathanTanker" type="SpaceAlert.Model.Menaces.MenaceExterne" singleton="false">
    <property name="Name" value="Leviathan Tanker"/>
    <property name="Shield" value="3"/>
    <property name="CurrentHp" value="8"/>
    <property name="MaxHp" value="8"/>
    <property name="Speed" value="2"/>
    <property name="AttackValues">
      <dictionary key-type="SpaceAlert.Model.Helpers.Enums.TypeCase">
        <entry key="X">
          <list element-type="int">
            <value>2</value>
          </list>
        </entry>
        <entry key="Y">
          <list element-type="int">
            <value>2</value>
          </list>
        </entry>
        <entry key="Z">
          <list element-type="int">
            <value>2</value>
          </list>
        </entry>
      </dictionary>
    </property>
    <property name="HealValues">
      <dictionary key-type="SpaceAlert.Model.Helpers.Enums.TypeCase">
        <entry key="Y" value="2" />
      </dictionary>
    </property>
    <property name="ActionsX">
      <list>
        <ref object="Attack" />
      </list>
    </property>
    <property name="ActionsY">
      <list>
        <ref object="Attack" />
        <ref object="Heal" />
      </list>
    </property>
    <property name="ActionsZ">
      <list>
        <ref object="Attack" />
      </list>
    </property>
  </object>
</objects>
